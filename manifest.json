{
  "manifest_version": 2,
  "name": "Квадратная аватарка",
  "version": "1.0",
  "description": "Делаем аватарку ВК квадратной",
  "permissions": [
    "privacy"
  ],
  "browser_action": {
  "default_popup": "index.html"

},
  "content_scripts": [
    {
      "matches": [ "https://vk.com/*" ],
      "js": [ "script.js" ]
    }
  ]
}